{
  "manifest_version": 2,
  "name": "Jump Jump",
  "version": "1.0",
  "description": "Redirect specific URL to new URL",
  "homepage_url": "https://github.com/g-tmp/jumpjump",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },

  "browser_action": {
    "default_icon": "Luci.png",
    "default_title": "Jump action",
    "default_popup": "options/options.html"
  },

  "icons": {
    "48": "Luci.png"
  }

}