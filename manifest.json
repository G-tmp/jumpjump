{
  "manifest_version": 2,
  "name": "Jump Jump",
  "version": "1.1",
  "description": "Redirect specific URL to new URL",
  "homepage_url": "https://github.com/g-tmp/jumpjump",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },

  "icons": {
    "48": "Luci.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "@jump-jump",
      "strict_min_version": "92.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}